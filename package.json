{
  "name": "react-redux-basic",
  "version": "1.0.0",
  "description": "The basic config for react project with redux",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "start": "NODE_ENV=dev webpack-dev-server --progress --colors --open --port 8081 & json-server --watch ./src/mock/mock.json --routes ./src/mock/routes.json --port 3000",
    "dev": "./node_modules/.bin/webpack-dev-server --content-base src --inline --hot --history-api-fallback",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "env",
      "stage-2",
      "react",
      "es2016"
    ],
    "plugins": [
      "transform-runtime",
      [
        "import",
        [
          {
            "libraryName": "antd",
            "style": "css"
          },
          {
            "libraryName": "antd-mobile",
            "style": "css"
          }
        ]
      ]
    ]
  },
  "author": "YUN",
  "license": "ISC",
  "dependencies": {
    "es6-promise": "^4.1.0",
    "history": "^4.6.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-pure-render-mixin": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.0",
    "redux-form": "^6.8.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-promise-middleware": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
